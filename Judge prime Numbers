
#include<stdio.h>

int main()//判断100到200之间有多少个素数
{
	int i = 0;
	int count = 0;
	for (i = 100; i <= 200; i++)
	{
		//判断i是否是素数
		//素数的判断规则，只能被1和本身整除的数
		//试除法
		//产生i到i-1试着去除自己本身
		int j = 0;
		for (j = 2; j < i; j++)
		{
			if (i%j == 0)
			{
				break;//out for
			}
		}
		if (j == i)
		{
			count++;
			printf("%d ", i);
		}
	}
	printf("\ncount = %d\n", count);
	return 0;
}
