int main()//找出两个数的较大值
{
	int x = 2;
	int y = 5;
	int sum = 0;
	sum = max(x, y);
	printf("%d\n", sum);
	return 0;
}
int get_max(int x, int y)
{
	//if (x > y)
	//	return x;
	//else
	//	return y;
	return (x > y) ? (x) : (y);
}
